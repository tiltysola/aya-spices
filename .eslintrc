{
  "plugins": ["simple-import-sort", "react-hooks"],
  "extends": "eslint-config-ali/typescript/react",
  "rules": {
    "simple-import-sort/imports": ["error", {
      "groups": [
        ["^react"],
        ["^"],
        ["^@"],
        ["^@/pages", "^@/components"],
        ["^\\."],
        ["^\\u0000"]
      ]
    }],
    "react/no-array-index-key": 0,
    "no-console": 0,
    "max-len": 0
  }
}
